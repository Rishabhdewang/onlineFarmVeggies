              "apps/admin/src/assets/css/quill.snow.css",
              "apps/admin/src/assets/css/quill.bubble.css",
              "apps/admin/src/assets/css/hopscotch.css",
              "apps/admin/src/assets/css/demo.css",
              "apps/admin/src/assets/sass/app.scss"
